<?xml version="1.0"?>
<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="verbose" value="true"/>
    <arg name="world_name" value="$(find prius_gazebo)/worlds/prius_city.world"/>
  </include>

  <group ns="prius_city">
    <include file="$(find prius_gazebo)/launch/prius.launch">
      <arg name="name" value="prius_city"/>
      <arg name="pos_x" value="1.45"/>
      <arg name="pos_y" value="30"/>
      <arg name="pos_z" value="0"/>
      <arg name="r" value="0"/>
      <arg name="p" value="0"/>
      <arg name="y" value="-1.57"/>
    </include>
  </group>

  <group ns="prius_simulate">
    <include file="$(find prius_gazebo)/launch/prius.launch">
      <arg name="name" value="prius_simulate"/>
      <arg name="pos_x" value="0"/>
      <arg name="pos_y" value="1.4"/>
      <arg name="pos_z" value="0"/>
      <arg name="r" value="0"/>
      <arg name="p" value="0"/>
      <arg name="y" value="0"/>
    </include>
  </group>

</launch>
